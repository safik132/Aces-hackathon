      "package": "com.safi11.Aceshackathon"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2789ca31-28e4-4989-9a7c-120dd0dd998c"
      }
    }
